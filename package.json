{
  "name": "cointrader-v3",
  "version": "0.0.1",
  "scripts": {
    "trade-watch": "node-sass --watch --output-style compressed scss/trade.scss build/trade.css",
    "styles-watch": "node-sass --watch --output-style compressed scss/marketing.scss build/marketing.css",
    "simple-ui-watch": "node-sass --watch --output-style compressed scss/simple-ui.scss build/simple-ui.css",
    "trade-styles": "node-sass --output-style compressed scss/trade.scss build/trade.css",
    "marketing-styles": "node-sass --output-style compressed scss/marketing.scss build/marketing.css",
    "simple-ui-styles": "node-sass --output-style compressed scss/simple-ui.scss build/simple-ui.css",
    "fx-blue-chart-styles": "node-sass --output-style compressed scss/fx-blue-charts.scss build/fx-blue-charts.css",
    "styles": "npm run trade-styles && npm run marketing-styles && npm run simple-ui-styles",
    "images": "cp -r images/ build/images",
    "copy-static-files": "cp -r src/lib build/lib",
    "js": "NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "start:dev": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "build": "npm run styles && npm run fx-blue-chart-styles && npm run js && npm run copy-static-files",
    "test": "jest --coverage --coverageDirectory=__tests_output__/coverage --silent",
    "test:watch": "jest --watch --verbose false",
    "command:translation:download": "npx babel-node commands/downloadTranslation.js",
    "lint": "eslint src/**",
    "deploy": "node scripts/deploy.js"
  },
  "dependencies": {
    "@aws-amplify/auth": "^1.2.29",
    "@babel/runtime": "^7.5.5",
    "@stomp/stompjs": "^5.3.0",
    "airbrake-js": "^1.5.0",
    "aws-sdk": "^2.479.0",
    "ethereum-address": "0.0.4",
    "file-saver": "^2.0.0",
    "fs": "0.0.1-security",
    "highcharts": "6.1.1",
    "moment": "^2.22.2",
    "multicoin-address-validator": "^0.3.10",
    "prop-types": "latest",
    "qrcode": "^1.3.3",
    "qrcode.react": "^0.9.2",
    "react": "^16.5.2",
    "react-click-outside": "^3.0.1",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.5.2",
    "react-dropzone": "^8.0.3",
    "react-easy-swipe": "0.0.18",
    "react-google-recaptcha": "^1.0.5",
    "react-perfect-scrollbar": "^1.5.0",
    "react-redux": "^5.0.7",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^4.3.1",
    "react-scrollchor": "^6.0.0",
    "react-slick": "^0.24.0",
    "react-switch": "^3.0.4",
    "react-toastify": "^4.4.3",
    "react-tooltip": "^3.9.2",
    "react-tradingview-widget": "^1.3.0",
    "react-transition-group": "^2.5.0",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.0",
    "redux-airbrake": "^2.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "request": "^2.88.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.1",
    "save-dev": "^2.0.0",
    "seedrandom": "^2.4.4",
    "uuid": "^3.3.2",
    "wallet-address-validator": "^0.2.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.0.0",
    "jest-localstorage-mock": "^2.4.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.11.0",
    "npx": "^10.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "react-test-renderer": "^16.4.1",
    "recursive-readdir": "^2.2.2",
    "redux-test-utils": "^0.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.0.7",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "optionalDependencies": {
    "fsevents": "^1.2.7"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
